        trigger ../prep/jgdas_prep == complete and ../prep/jgdas_emcsfc_sfc_prep == complete and /prod06/gdas/enkf/jgdas_enkf_post == complete
        event 1 release_fcst
    endfamily # /prod12/gdas/analysis
